function Main.main 0
	push constant 0
	pop local 0
	push argument 1
	pop local 1  
label loop
	push constant 0
	push local 1
	eq
	if-goto end
	push local 0
	push argument 0
	sub
	pop local 0
	push local 1
	push constant 1
	sub
	pop local 1
	goto loop
label end
	push local 0
	pop temp 0
	return
	
